{% set bodyclass = 'page-booking-visite' %}
{# toolbar-fixed#}
{% include '../header.twig' with bodyclass %}

<div class="clearfix wrapper-fullwidth">
    <div class="content-main-title">
        <div class="wrapper">
            <div class="main-title">
                <h2 class="title">Programmez votre visite</h2>
                <div class="des">Choisissez la date et l’heure puis remplissez le formulaire</div>
                <a href="" class="btn btn-back"><i class="icon-arrow-left"></i>Retour</a>
            </div>

        </div>
    </div>
    <div class="content-booking">
        <div class="wrapper">
            <div class="booking-row">
                <div class="booking-row-left">
                    <div class="booking-day">

                        <div class="booking-slide">
                            <div class="slideday owl-carousel owl-theme">
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Mer</div>
                                        <div class="daymiddle">13</div>
                                        <div class="daybottom">Déc.</div>
                                    </div>

                                </div>
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Jeu</div>
                                        <div class="daymiddle">14</div>
                                        <div class="daybottom">Déc.</div>
                                    </div>

                                </div>
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Ven</div>
                                        <div class="daymiddle">15</div>
                                        <div class="daybottom">Déc.</div>
                                    </div>

                                </div>
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Sam</div>
                                        <div class="daymiddle">16</div>
                                        <div class="daybottom">Déc.</div>
                                    </div>

                                </div>
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Dim</div>
                                        <div class="daymiddle">17</div>
                                        <div class="daybottom">Déc.</div>
                                    </div>

                                </div>
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Lun</div>
                                        <div class="daymiddle">18</div>
                                        <div class="daybottom">Déc.</div>
                                    </div>

                                </div>
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Lun</div>
                                        <div class="daymiddle">19</div>
                                        <div class="daybottom">Déc.</div>
                                    </div>

                                </div>
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Lun</div>
                                        <div class="daymiddle">20</div>
                                        <div class="daybottom">Déc.</div>
                                    </div>

                                </div>
                                <div class="itemday">
                                    <div class="itemday__inner">
                                        <div class="daytop">Lun</div>
                                        <div class="daymiddle">21</div>
                                        <div class="daybottom">Déc.</div>
